# Environment variables for AI Debugger Factory
# Render deployment configuration

# Database
DATABASE_URL=postgres://postgres:postgres@localhost:5432/ai_debugger_factory

# Application settings
ENVIRONMENT=production
DEBUG=False
LOG_LEVEL=INFO

# API settings
API_VERSION=v1
API_PREFIX=/api/v1

# Security
SECRET_KEY=your-production-secret-key-here
